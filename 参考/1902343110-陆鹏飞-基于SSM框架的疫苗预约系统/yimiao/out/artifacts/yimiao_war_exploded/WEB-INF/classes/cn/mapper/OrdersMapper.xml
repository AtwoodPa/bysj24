<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mapper.OrdersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.entity.Orders">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="cid" property="cid" />
        <result column="yid" property="yid" />
        <result column="time" property="time" />
        <result column="no" property="no" />
        <result column="status" property="status" />
        <result column="isdel" property="isdel" />
        <result column="bak1" property="bak1" />
        <result column="bak2" property="bak2" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, cid, yid, time, no, status, isdel, bak1, bak2
    </sql>

</mapper>
