<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mapper.RecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.entity.Record">
        <id column="id" property="id" />
        <result column="oid" property="oid" />
        <result column="uid" property="uid" />
        <result column="cid" property="cid" />
        <result column="yid" property="yid" />
        <result column="position" property="position" />
        <result column="mode" property="mode" />
        <result column="doctor" property="doctor" />
        <result column="isdel" property="isdel" />
        <result column="bak1" property="bak1" />
        <result column="bak2" property="bak2" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, oid, uid, cid, yid, position, mode, doctor, isdel, bak1, bak2
    </sql>

</mapper>
