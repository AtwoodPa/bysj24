<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ym.vaccine.mapper.YmInoculateSiteMapper">

    <resultMap type="com.ym.vaccine.domain.YmInoculateSite" id="YmInoculateSiteResult">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="address" column="address"/>
        <result property="imgUrl" column="img_url"/>
    </resultMap>
    <select id="selectInoculateSiteByName" resultType="com.ym.vaccine.domain.YmInoculateSite">
        SELECT * FROM ym_inoculate_site WHERE name = #{name}
    </select>


</mapper>
